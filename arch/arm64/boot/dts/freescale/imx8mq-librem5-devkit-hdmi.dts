/dts-v1/;

#include "imx8mq-librem5-devkit.dts"

/delete-node/ &dcss_dsi_out;
/delete-node/ &mipi_dsi_dcss_in;
&dcss {
	status = "okay";

	port@0 {
		dcss_hdmi_out: endpoint {
			remote-endpoint = <&hdmi_in>;
		};
	};
};

&hdmi {
	compatible = "nxp,imx8mq-cdns-hdmi";
	lane-mapping = <0xe4>;
	status = "okay";
	port@0 {
		hdmi_in: endpoint {
			remote-endpoint = <&dcss_hdmi_out>;
		};
	};
};
